{
  "GlobalConfiguration": {
    "BaseUrl": "https://localhost:8000"
  },
  "Routes": [
    {
      "UpstreamPathTemplate": "/api/authenticate/login",
      "UpstreamHttpMethod": [ "POST" ],
      "DownstreamPathTemplate": "/api/authenticate/login",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 8001
        }
      ]
    },
    {
      "UpstreamPathTemplate": "/api/authenticate/register",
      "UpstreamHttpMethod": [ "POST" ],
      "DownstreamPathTemplate": "/api/authenticate/register",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 8001
        }
      ]
    },
    {
      "UpstreamPathTemplate": "/api/authenticate/validate",
      "UpstreamHttpMethod": [ "GET" ],
      "DownstreamPathTemplate": "/api/authenticate/validate",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 8001
        }
      ],
    },
    {
      "UpstreamPathTemplate": "/api/projects/{something}",
      "UpstreamHttpMethod": [
        "GET",
        "POST",
        "PUT",
        "DELETE"
      ],
      "DownstreamPathTemplate": "/api/projects/{something}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 8002
        }
      ]
    }
  ]
}